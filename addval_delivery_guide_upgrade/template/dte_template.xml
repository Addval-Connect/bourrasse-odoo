<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="dte_subtemplate" inherit_id="l10n_cl_edi_stock.dte_subtemplate">
            <xpath expr="//RznSoc" position="replace">                
                <t t-if="picking.company_id.partner_id.parent_name">
                    <RznSoc t-esc="format_length(picking.company_id.partner_id.parent_name, 100)" />
                </t>
                <t t-if="not picking.company_id.partner_id.parent_name">
                    <RznSoc t-esc="format_length(picking.company_id.partner_id.name, 100)" />
                </t>
            </xpath>
        </template>

        <template id="documento_dte">
            <Documento t-att-ID="doc_id">
                <t t-call="addval_delivery_guide_upgrade.dte_subtemplate"/>
            </Documento>
        </template>

        <template id="dte_template" inherit_id="l10n_cl_edi_stock.dte_template">
            <xpath expr="//t[@t-call='l10n_cl_edi_stock.dte_subtemplate']" position="replace">
                <t t-call="addval_delivery_guide_upgrade.dte_subtemplate"/>
            </xpath>
            <xpath expr="//t[@t-call='l10n_cl_edi_stock.documento_dte']" position="replace">
                <t t-call="addval_delivery_guide_upgrade.documento_dte"/>
            </xpath>
        </template>
    </data>
</odoo>